{
  "name": "@phonebook/ui",
  "version": "1.0.0",
  "description": "phonebook-ui",
  "main": "src/main.js",
  "scripts": {
    "jest": "jest --silent",
    "test:e2e": "cypress run ",
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "build": "rimraf ./public/static/build/ && cross-env NODE_ENV=production webpack --config webpack-production.config.js",
    "lint": "eslint src --ext js,jsx",
    "test": "npm run lint",
    "extract:po": "bash bin/extract/po_to_json.sh",
    "extract:json": "bash bin/extract/json_to_po.sh"
  },
  "repository": {
    "type": "git"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.svg$": "jest-transform-stub",
      "^.+\\.(css|less)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@dicebear/avatars": "^4.10.3",
