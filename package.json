    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "react-refresh": "^0.14.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
