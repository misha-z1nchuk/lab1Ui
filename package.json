    "antd": "^4.21.6",
    "awesome-debounce-promise": "^2.1.0",
    "classnames": "^2.3.1",
    "deep-equal": "^2.0.5",
    "echarts": "^5.3.3",
    "echarts-for-react": "^3.0.2",
    "fast-memoize": "^2.5.2",
    "file-saver": "^2.0.5",
    "history": "^5.3.0",
    "i18next": "^21.9.1",
    "inputmask": "^5.0.7",
    "js-cookie": "^3.0.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.18.6",
    "react-input-emoji": "^4.2.0",
    "react-international-phone": "^1.8.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "react-spinners": "^0.13.7",
    "redux": "^4.2.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.1",
    "regionist": "^3.2.1",
    "ws": "^8.17.0",
    "xlsx": "^0.18.5"
